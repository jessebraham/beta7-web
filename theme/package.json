{
  "private": true,
  "browserslist": [
    "last 2 versions",
    "> 1%"
  ],
  "scripts": {
    "clean": "rm static/css/*",
    "start": "postcss ./styles --env=development --dir ./static/css --watch",
    "dev": "postcss ./styles --env=development --dir ./static/css",
    "prod": "postcss ./styles --env=production --dir ./static/css --map"
  },
  "dependencies": {
    "tailwindcss": "^1.1.4"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "autoprefixer": "^9.7.3",
    "cssnano": "^4.1.10",
    "postcss-cli": "^6.1.3",
    "postcss-hash": "^1.0.2",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.2.1"
  }
}
